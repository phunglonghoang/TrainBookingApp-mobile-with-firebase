<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#53C4E6">


    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="93dp"
        android:text="Thông tin tài khoản"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="371dp"
        android:layout_height="411dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="11dp"
        android:background="@drawable/bg_reset_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8">

        <EditText
            android:id="@+id/etEmail_EditProfileAdmin"
            android:layout_width="243dp"
            android:layout_height="52dp"
            android:layout_marginEnd="17dp"
            android:layout_marginBottom="65dp"
            android:drawableLeft="@drawable/ic_email"
            android:ems="10"
            android:focusable="false"
            android:inputType="none"
            android:textAlignment="center"
            android:textColor="#1347B7"
            app:layout_constraintBottom_toTopOf="@+id/btnUpdateProfileAdmin"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="75dp"
            android:text="Email:"
            android:textAlignment="center"
            android:textColor="#1347B7"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/btnUpdateProfileAdmin"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/btnUpdateProfileAdmin"
            android:layout_width="332dp"
            android:layout_height="55dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="10dp"
            android:backgroundTint="#AC8E8E"
            android:text="Cập nhật"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/txtChangePasswordAdmin"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/txtChangePasswordAdmin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="16dp"
            android:text="Đổi mật khẩu?"
            android:textAlignment="center"
            android:textColor="#31578F"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <EditText
            android:id="@+id/etNgaySinh_EditProfileAdmin"
            android:layout_width="243dp"
            android:layout_height="52dp"
            android:layout_marginEnd="17dp"
            android:layout_marginBottom="30dp"
            android:clickable="true"
            android:drawableLeft="@drawable/ic_edit_calendar"
            android:ems="10"
            android:focusable="false"
            android:inputType="none"
            android:textAlignment="center"
            android:textColor="#1347B7"
            app:layout_constraintBottom_toTopOf="@+id/etEmail_EditProfileAdmin"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="55dp"
            android:text="Ngày sinh:"
            android:textAlignment="center"
            android:textColor="#1347B7"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView19"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="55dp"
            android:text="Tài khoản:"
            android:textAlignment="center"
            android:textColor="#1347B7"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/etName_EditProfileAdmin"
            android:layout_width="243dp"
            android:layout_height="52dp"
            android:layout_marginEnd="17dp"
            android:layout_marginBottom="30dp"
            android:drawableLeft="@drawable/ic_assignment_ind"
            android:ems="10"
            android:inputType="text"
            android:textAlignment="center"
            android:textColor="#1347B7"
            app:layout_constraintBottom_toTopOf="@+id/etNgaySinh_EditProfileAdmin"
            app:layout_constraintEnd_toEndOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>